@page
@using Microsoft.Extensions.Localization
@using Microsoft.AspNetCore.StaticFiles.Infrastructure
@model LockoutModel
@inject IStringLocalizer<SharedOptions> SharedLocalizer
@{
    ViewData["Title"] = "Locked out";
}

<header>
    <div class="container">
        <h1 class="text-danger">@SharedLocalizer["Locked out"]</h1>
        <p class="text-danger">@SharedLocalizer["Your account has been locked out, please try again later."]</p>
    </div>
</header>
